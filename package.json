{
  "name": "immutable-class",
  "version": "0.8.1",
  "description": "A template for creating immutable classes",
  "keywords": [
    "immutable",
    "class"
  ],
  "author": {
    "name": "Vadim Ogievetsky",
    "url": "http://vadim.ogievetsky.com"
  },
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "http://github.com/implydata/immutable-class.git"
  },
  "jest": {
    "preset": "ts-jest",
    "testMatch": [
      "**/?(*.)+(spec).ts?(x)"
    ]
  },
  "main": "build/index.js",
  "typings": "build/index.d.ts",
  "files": [
    "build/"
  ],
  "scripts": {
    "prepare": "tdi -q",
    "compile": "tsc",
    "pretest": "tsc",
    "test": "./run-tests",
    "watch": "tsc --watch",
    "tslint": "./node_modules/.bin/tslint -c tslint.json --project tsconfig.json --formatters-dir ./node_modules/awesome-code-style/formatter 'src/**/*.ts?(x)'",
    "tslint-fix": "npm run tslint -- --fix",
    "tslint-changed-only": "git diff --cached --diff-filter=ACMR --name-only | grep -E \\.tsx\\?$ | xargs ./node_modules/.bin/tslint -c tslint.json --project tsconfig.json --formatters-dir ./node_modules/awesome-code-style/formatter",
    "tslint-fix-changed-only": "npm run tslint-changed-only -- --fix"
  },
  "dependencies": {
    "has-own-prop": "~2.0.0",
    "tslib": "~1.10.0"
  },
  "devDependencies": {
    "@types/jest": "^24.0.15",
    "awesome-code-style": "^1.4.2",
    "chai": "4.2.0",
    "immutable-class-tester": "0.6.0",
    "jest": "^24.8.0",
    "mocha": "6.1.4",
    "tdi": "0.7.7",
    "ts-jest": "^24.0.2",
    "ts-node": "8.3.0",
    "typescript": "3.5.2"
  }
}
