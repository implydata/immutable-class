{
  "name": "immutable-class",
  "version": "0.11.3",
  "description": "A template for creating immutable classes",
  "keywords": [
    "immutable",
    "class"
  ],
  "author": {
    "name": "Vadim Ogievetsky",
    "url": "http://vadim.ogievetsky.com"
  },
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "http://github.com/implydata/immutable-class.git",
    "directory": "packages/immutable-class"
  },
  "jest": {
    "preset": "ts-jest",
    "testMatch": [
      "**/?(*.)+(spec).ts?(x)"
    ]
  },
  "main": "build/index.js",
  "module": "lib/index.js",
  "typings": "types/index.d.ts",
  "scripts": {
    "compile": "tsc --module commonjs && tsc --module esnext --outDir lib/",
    "prepublishOnly": "npm run compile",
    "test": "jest --silent 2>&1",
    "watch": "tsc --watch"
  },
  "dependencies": {
    "has-own-prop": "^2.0.0",
    "tslib": "^2.6.2"
  },
  "devDependencies": {
    "@types/jest": "^29.5.13",
    "immutable-class-tester": "^0.7.5",
    "jest": "^29.7.0",
    "ts-jest": "^29.2.5",
    "typescript": "^5.6.3"
  }
}
