{
  "name": "eslint-plugin-immutable-class",
  "version": "1.0.0",
  "description": "ESLint plugin for immutable-class",
  "keywords": [
    "immutable",
    "class",
    "eslint",
    "eslintplugin"
  ],
  "author": {
    "name": "John Gozde",
    "url": "https://github.com/jgoz/"
  },
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "http://github.com/implydata/immutable-class.git",
    "directory": "packages/eslint-plugin-immutable-class"
  },
  "main": "build/index.js",
  "scripts": {
    "compile": "tsc",
    "prepublishOnly": "npm run compile",
    "test": "jest --silent 2>&1",
    "watch": "tsc --watch"
  },
  "jest": {
    "testMatch": [
      "**/?(*.)+(spec).ts?(x)"
    ],
    "transform": {
      "^.+\\.(t|j)sx?$": "@swc/jest"
    }
  },
  "dependencies": {
    "@typescript-eslint/utils": "^6.0.0"
  },
  "peerDependencies": {
    "eslint": ">=0.8.0"
  },
  "devDependencies": {
    "@swc/core": "^1.3.83",
    "@swc/jest": "^0.2.29",
    "@typescript-eslint/parser": "^6.0.0",
    "@typescript-eslint/rule-tester": "^6.0.0",
    "@typescript-eslint/types": "^6.0.0",
    "@types/jest": "^29.4.0",
    "eslint": "^8.48.0",
    "jest": "^29.4.3",
    "typescript": "^5.2.2"
  }
}
